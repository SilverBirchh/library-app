<h1>Feedback</h1>

<table class="table table-bordered table-striped">
    <thead>
      <tr>
          <th>ID</th>
          <th>E-mail</th>
          <th>Message</th>
      </tr>
    </thead>
    <tbody>
    {{#each model as |feedback|}}
        <tr>
            <th>{{feedback.id}}</th>
            <td>{{feedback.email}}</td>
            <td>{{feedback.message}}</td>
        </tr>
    {{/each}}

    </tbody>
</table>
